            font-size: 12px;
            line-height: 1.5;
            border-radius: 3px;
        }

        p.form-control {
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            display: block;
            width: 100%;
            background-image: none;
            color: #282323;
            outline: 0;
            height: 16px;
            padding: 6px 11px !important;
            line-height: calc(16px + 6px);
            font-size: 13px;
            font-weight: normal;
            vertical-align: top;
            background-color: #ffffff;
            min-height: 37px;
            box-shadow: none !important;
            border-radius: 0px;
            border-radius: 2px;
            border: 1px solid #e5e9ec;
            transition: background 0.2s linear 0s, box-shadow 0.2s linear 0s;
        }

        * {
            user-select: none;
        }

        td:not([class*="operate-cell"]),
        td:not([class*="operate-cell"]) a {
            user-select: text;
        }
    </style>

